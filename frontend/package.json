{
    "name": "frontend",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prepare": "cd .. && npx husky install frontend/.husky",
        "test": "jest",
        "test-watch": "jest --watchAll",
        "prettier": "prettier --write index.html package.json \"src/**/*.{js,jsx,ts,tsx,css,json,svg}\""
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,json,md,svg}": [
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --rule react-hooks/exhaustive-deps:off"
        ]
    },
    "dependencies": {
        "@navikt/ds-icons": "^0.5.2",
        "classnames": "^2.3.1",
        "date-fns": "^2.23.0",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-json-view": "^1.21.3",
        "react-query": "^3.23.0",
        "recoil": "^0.4.1"
    },
    "devDependencies": {
        "@testing-library/dom": "^8.3.0",
        "@testing-library/react": "^12.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/jest": "^27.0.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@vitejs/plugin-react-refresh": "^1.3.1",
        "esbuild": "^0.12.25",
        "esbuild-jest": "^0.5.0",
        "fetch-mock": "^9.11.0",
        "husky": "^7.0.2",
        "jest": "^27.1.1",
        "node-fetch": "^2.6.7",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.5",
        "typescript": "^4.3.2",
        "vite": "^2.5.4"
    }
}
