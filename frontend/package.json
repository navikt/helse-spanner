{
    "name": "frontend",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prepare": "cd .. && npx husky install frontend/.husky",
        "test": "jest",
        "test-watch": "jest --watchAll",
        "prettier": "prettier --write index.html package.json \"src/**/*.{js,jsx,ts,tsx,css,json,svg}\""
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,json,md,svg}": [
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --rule react-hooks/exhaustive-deps:off"
        ]
    },
    "dependencies": {
        "@navikt/ds-css": "^5.12.2",
        "@navikt/ds-icons": "^0.5.2",
        "@navikt/ds-react": "^5.12.2",
        "classnames": "^2.3.1",
        "date-fns": "^2.23.0",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-json-view": "^1.21.3",
        "react-query": "^3.23.0",
        "react-router-dom": "^6.11.0",
        "recoil": "^0.4.1"
    },
    "devDependencies": {
        "@testing-library/dom": "^8.3.0",
        "@testing-library/react": "^12.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/jest": "^27.0.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@vitejs/plugin-react-refresh": "^1.3.1",
        "esbuild": "^0.25.2",
        "esbuild-jest": "^0.4.0",
        "fetch-mock": "^12.5.2",
        "husky": "^7.0.2",
        "jest": "^29.7.0",
        "node-fetch": "^3.3.2",
        "prettier": "^3.5.3",
        "ts-jest": "^29.3.1",
        "typescript": "^5.8.3",
        "vite": "6.2.5"
    }
}
