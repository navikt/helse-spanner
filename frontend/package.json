{
    "name": "frontend",
    "version": "0.0.0",
    "engines": {
        "node": "24.x",
        "pnpm": "10"
    },
    "packageManager": "pnpm@10.24.0",
    "scripts": {
        "dev": "vite",
        "start": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "prepare": "cd .. && npx husky install frontend/.husky",
        "test": "vitest run",
        "test-watch": "vitest watch",
        "prettier": "prettier --write index.html package.json \"src/**/*.{js,jsx,ts,tsx,css,json,svg}\""
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,json,md,svg}": [
            "prettier --write"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix --rule react-hooks/exhaustive-deps:off"
        ]
    },
    "dependencies": {
        "@microlink/react-json-view": "^1.21.3",
        "@navikt/ds-css": "^7.35.1",
        "@navikt/ds-icons": "^3.4.3",
        "@navikt/ds-react": "^7.35.1",
        "classnames": "^2.3.1",
        "date-fns": "^2.23.0",
        "react": "^18.0.0",
        "react-dom": "^18.3.1",
        "react-query": "^3.39.3",
        "react-router-dom": "^7.10.1",
        "recoil": "^0.7.7"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/react": "^18.2.7",
        "@types/react-dom": "^18.0.0",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/ui": "^4.0.15",
        "esbuild": "^0.25.0",
        "fetch-mock": "^9.11.0",
        "husky": "^7.0.2",
        "jsdom": "^27.3.0",
        "node-fetch": "^2.6.7",
        "prettier": "^2.3.2",
        "typescript": "^5.9.3",
        "vite": "^7.2.7",
        "vitest": "^4.0.15"
    }
}
